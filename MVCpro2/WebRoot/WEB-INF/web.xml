<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>MVCpro2</display-name>
  
  <filter>
  			<filter-name>FreeLogin</filter-name>
  			<filter-class>filter.FreeLogin</filter-class>
  			<init-param>
  				<param-name>notCheckPath</param-name>
  				<param-value>/error.jsp,/login.do,/out.do,/Saveout.do,/CreateVerifyImage.do</param-value>
  			</init-param>
  </filter>
  
  <filter-mapping>
  		<filter-name>FreeLogin</filter-name>
  		<url-pattern>*.jsp</url-pattern>
  		<url-pattern>*.html</url-pattern>
  		<url-pattern>*.java</url-pattern>
  		 <url-pattern>*.do</url-pattern>
  		 <url-pattern>*</url-pattern>
  </filter-mapping>
  
  <filter>
  			<filter-name>DifferentLogin</filter-name>
  			<filter-class>filter.DifferentLogin</filter-class>
  			<init-param>
  				<param-name>notCheckPath</param-name>
  				<param-value>/error.jsp,/out.do,/Saveout.do,/CreateVerifyImage.do</param-value>
  			</init-param>
  </filter>
  
  <filter-mapping>
  		<filter-name>DifferentLogin</filter-name>
  		<url-pattern>*.jsp</url-pattern>
  		<url-pattern>*.html</url-pattern>
  		<url-pattern>*.java</url-pattern>
  		 <url-pattern>*.do</url-pattern>
  		 <url-pattern>*</url-pattern>
  </filter-mapping>
 
 
 	 <filter>
  			<filter-name>IsIfLogin</filter-name>
  			<filter-class>filter.IsIfLogin</filter-class>
  			<init-param>
  				<param-name>notCheckPath</param-name>
  				<param-value>/error.jsp,/login.do,/out.do,/Saveout.do,/servlet/CreateImage,/main.jsp,/index.html,/login.html</param-value>
  			</init-param>
  </filter>
  
  <filter-mapping>
  		<filter-name>IsIfLogin</filter-name>
  		<url-pattern>*.jsp</url-pattern>
  		<url-pattern>*.html</url-pattern>
  		<url-pattern>*.java</url-pattern>
  		 <url-pattern>*.do</url-pattern>
  		 <url-pattern>*</url-pattern>
  </filter-mapping>
 
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>controller.login</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>CreateVerifyImage</servlet-name>
    <servlet-class>controller.CreateVerifyImage</servlet-class>
  </servlet>
  
  
  <servlet>
    <servlet-name>GetDownloadList</servlet-name>
    <servlet-class>controller.GetDownloadList</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>out</servlet-name>
    <servlet-class>controller.out</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DownloadController</servlet-name>
    <servlet-class>controller.DownloadController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Saveout</servlet-name>
    <servlet-class>controller.Saveout</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetUserList</servlet-name>
    <servlet-class>controller.GetUserList</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetDownloadList2</servlet-name>
    <servlet-class>controller.GetDownloadList2</servlet-class>
  </servlet>








  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/servlet/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CreateVerifyImage</servlet-name>
        <url-pattern>/servlet/CreateImage</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetDownloadList</servlet-name>
    <url-pattern>/servlet/GetDownloadList</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>out</servlet-name>
    <url-pattern>/servlet/out</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DownloadController</servlet-name>
    <url-pattern>/servlet/DownloadController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Saveout</servlet-name>
    <url-pattern>/servlet/Saveout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetUserList</servlet-name>
    <url-pattern>/servlet/GetUserList</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetDownloadList2</servlet-name>
    <url-pattern>/servlet/GetDownloadList2</url-pattern>
  </servlet-mapping> 
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>